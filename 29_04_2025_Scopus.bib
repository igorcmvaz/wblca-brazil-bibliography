@ARTICLE{de Lara2024,
	author = {de Lara, Beatriz Leão Evangelista and Penteado, Carmenlucia Santos Giordano},
	title = {Environmental assessment of construction waste prevention: A case study in a social housing project in Southeast Brazil},
	year = {2024},
