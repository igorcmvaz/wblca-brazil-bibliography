@ARTICLE{Benitez2017,
	author = {Benitez, Aline do Nascimento and Martins, Felippe Danyel Cardoso and Mareze, Marcelle and Santos, Nelson Jessé Rodrigues and Ferreira, Fernanda Pinto and Martins, Camila Marinelli and Garcia, João Luis and Mitsuka-Breganó, Regina and Freire, Roberta Lemos and Biondo, Alexander Welker and Navarro, Italmar Teodorico},
	title = {Spatial and simultaneous representative seroprevalence of anti-Toxoplasma gondii antibodies in owners and their domiciled dogs in a major city of southern Brazil},
	year = {2017},
	journal = {PLoS ONE},
