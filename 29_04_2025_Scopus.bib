	volume = {251},
	doi = {10.1016/j.jclepro.2019.119649},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85076575249&doi=10.1016%2fj.jclepro.2019.119649&partnerID=40&md5=276a2d64b5e471715700faf9f3b418f7},
