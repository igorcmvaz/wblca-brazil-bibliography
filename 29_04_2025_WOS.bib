@article{ WOS:000338802400038,
Author = {da Silva Pereira, Edinaldo Jose and Pinho, Joao Tavares and Barros
   Galhardo, Marcos Andre and Macedo, Wilson Negrao},
Title = {Methodology of risk analysis by Monte Carlo Method applied to power
   generation with renewable energy},
