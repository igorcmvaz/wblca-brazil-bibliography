@article{ WOS:001393372800001,
Author = {Correia, Isadora Bruna and Miyasaka, Elza Luli and Cordoba, Rodrigo
   Eduardo and Cramolichi, Stanley Cabral and Neto, Jose da Costa Marques},
Title = {Qualitative and quantitative analysis of construction and demolition
   waste: case studies in commercial buildings in the San Francisco region
   - United States},
Journal = {ENGENHARIA SANITARIA E AMBIENTAL},
Year = {2024},
Volume = {29},
Abstract = {Civil construction is one of the main generators of solid waste since
   the process of dismantling and demolition occurs frequently and, in most
   cases, without adequate planning. The aim of this paper is to analyze
   the dismounting process of two commercial buildings in San Francisco Bay
   (California, United States). For this study, a case study was conducted
   with two partial demolition projects. The first phase aimed to identify
   and analyze the management of recycling and reuse. The second phase
   focused on studying the deconstruction project and characterizing the
   deconstruction. Finally, the last phase quantified and constructed
   indicators on the recycled or reused materials. Results identified that
   developments 1 and 2 generated construction and demolition waste (CDW)
   of 63.7 and 21.5 kg/m2 during the deconstruction process, with recycling
   rates of 9.8\% and 81.3\%, respectively. Despite the legislation
   requiring the redirection of these materials, California, as well as
   Brazil, still lacks specific aspects in the legislation regarding waste,
   to encourage and require that these issues be considered still in the
   initial design phase of the construction.},
