@inproceedings{ WOS:000385237800035,
Author = {Stiegert, I. and Hippert, A. and Borges, M.},
Editor = {Arezes, PM and Baptista, JS and Barroso, MP and Carneiro, P and Cordeiro, P and Costa, N and Melo, RB and Miguel, AS and Perestrelo, G},
Title = {Design for Maintenance guidelines: Safety height work},
Booktitle = {OCCUPATIONAL SAFETY AND HYGIENE IV},
Year = {2016},
Pages = {173-177},
Note = {12th International Symposium on Occupational Safety and Hygiene of
   Portuguese-Society-of-Occupational-Safety-and-Hygiene(SHO), Guimaraes,
   PORTUGAL, MAR 23-24, 2016},
Organization = {Portuguese Soc Occupat Safety \& Hygiene; Univ Minho, Sch Engn; Univ
   Porto, Fac Engn; Univ Lisbon, Fac Human Kinet; Polytechn Univ Catalonia;
   Tech Univ Delft; Faculdade Motricidade Humana; DELTA; EXIM; ACT;
   European Agcy Safety \& Hlth Work; Lavoro},
Abstract = {The traditional paradigm of civil construction design process has been
   characterised predominantly by linear logic with the absence of any
   integration among the different parts that constitute the life cycle of
   an enterprise. This scenerio begins at the design stage, resulting from
   the neglect of its management, in such a way that planning and
   controlling are replaced by chaos and improvisation. Many design-related
   deficiencies are due to the lack of communication among designers and
   little collaboration from the involved agents. As a response for this
   situation, the principles of Concurrent Engineering and of Design for
   Excellence (DFE) methods should be taken into consideration. DFE
   includes Design for Maintenance (DFM) practices, which are currently a
   relevant subject for the building sector, as buildings are expected to
   last for decades and the necessity of adaptation throughout their
   operational lives (including the reduction of energy consumption and
   bioclimatic demands) is very clear. Considering that maintenance
   includes several different factors and agents, this research aims to
   provide guidelines for occupational safety at heights during the
   maintenance stage of an enterprise, hoping to avoid accidents at the
   workplace-which is an important matter in the contemporary Brazilian
   context. For this purpose, a bibliographical revision was made from both
   national and international sources. As a result, we hope to contribute
   for the design process in the civil construction field. The diffusion of
   this subject's importance among the professionals in this field was
   perceived as necessary for an effective contribution to design
   practices, since these guidelines are not mandatory in the current
   legislation, although they would be a differential for the design
   process and the enterprise as a whole.},
