@article{ WOS:000434384500001,
Author = {Azevedo, Leticia Henrique and Ferreira, Murilo Prudente and Castilho,
   Raphael de Campos and Duarte Cancadoc, Paulo Henrique and de Moraesa,
   Gilberto Jose},
Title = {Potential of Macrocheles species (Acari: Mesostigmata:
   Macrochelidae) as control agents of harmful flies (Diptera) and biology
