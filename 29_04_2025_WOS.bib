@article{ WOS:000257592100002,
Author = {Favaro, Eliane A. and Mondini, Adriano and Dibo, Margareth R. and
   Barbosa, Angelita A. C. and Eiras, Alvaro E. and Neto, Francisco
   Chiaravalloti},
Title = {Assessment of entomological indicators of Aedes aegypti (L.) from
   adult and egg collections in Sao Paulo, Brazil},
Journal = {JOURNAL OF VECTOR ECOLOGY},
Year = {2008},
Volume = {33},
Number = {1},
Pages = {8-16},
Month = {JUN},
Abstract = {We compared the presence and mean number of eggs from oviposition traps
   with the mean number of Aedes aegypti females captured by manual
   aspirators and by MosquiTRAPs (TM) to determine the sensitivity of each
   method and to standardize each indicator. The collections of adults and
   eggs were performed over 23 weeks in six neighborhoods of Mirassol,
   state of Sao Paulo, Brazil. A better assessment of indicators required
   larger number of MosquiTRAPs, but to quantify the number of females per
   house, one trap was sufficient. The sensitivities of MosquiTRAPs and
   manual aspirations to detect the presence of A. aegypti females were
   similar, but were lower compared to oviposition traps. The correlation
   coefficients between the number of females captured by MosquiTRAPs and
   manual aspirations and the number of eggs from oviposition traps were
   low, which may be a consequence of each method showing different stages
   of the mosquito life cycle.},
