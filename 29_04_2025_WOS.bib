@article{ WOS:000487231200098,
Author = {Morales, Michele and Moraga, Gustavo and Kirchheim, Ana Paula and
   Passuello, Ana},
Title = {Regionalized inventory data in LCA of public housing: A comparison
   between two conventional typologies in southern Brazil},
Journal = {JOURNAL OF CLEANER PRODUCTION},
Year = {2019},
Volume = {238},
Month = {NOV 20},
Abstract = {Life Cycle Assessment (LCA) can provide the environmental impact of
   buildings from its initial construction until its demolition. Of
   particular interest are the environmental impacts of public housing as
   developing economies are facing the growing need for housing. However,
   the validity of the LCA results is directly related to how
   representative is the inventory data to regional characteristics.
   Existing LCA of buildings in Brazil frequently ignores regional data
   when compiling the LCA and there is a lack of study on this topic. This
   study performs an LCA that compares two types of public housing in
   southern Brazil: single-family dwellings (house condominium) and
   multi-family dwellings (apartment condominium). The unit areas compared
   are of similar size, and the construction methods and materials used are
   also similar. The LCA was performed using regionalized and
   non-regionalized inventory data based on ISO 14040/44 and EN 15804
   (modules A to C). Data from the Ecoinvent database v.3.3 was used with
   adaptations that reflected regional characteristics of Brazilian
   construction practices. Results show that the use stage (module B) is
   the most relevant in all environmental impact categories considered,
   particularly relevant are the electricity consumed (module B6) and
   replacement of constructions materials (module B4). Despite similar
   overall impacts, the single-family dwelling exhibited higher impact
   numbers compared to the multi-family dwelling, largely due to design and
   construction characteristics (module A). Regionalized data strongly
   influenced the results when compared to non-regionalized background
   data; therefore, the results obtained using non-regionalized data may
   lead to inconsistent decision-making. This study is one of the first LCA
   of buildings in Brazil using inventory data regionalized to local
   conditions and demonstrates the importance of regionalization since
   environmental impacts can vary significantly. (C) 2019 Elsevier Ltd. All
   rights reserved.},
